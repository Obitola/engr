% Force vs Distance

clear;
clc;
coef1 = [480:600];
for i = [1:121]
    func1(i) = coef1(i)*8/(588*sin(pi/6)+211.68*cos(pi/6));
end
figure(1);


coef2 = [480:600];
for i = [1:121]
    func2(i) = coef2(i)*8/(588*sin(pi/6)+246.96*cos(pi/6));
end
figure(1);
plot(coef1,func1,coef2,func2);

xlabel("Exerted Force(Newtons)");
ylabel("Maximum Distance(Meters)");
title('Exerted Force vs Maximum Distance');
legend("Friction coefficient = .36","Friction coefficient = .42");