clear;
clc;
unit = 1:121;
force = 480:1:600;
friction = 0.36:0.0005:0.42;

i = 1;
for x = force
    d1(i) = distance(x, 0.36);
    d2(i) = distance(x, 0.42);
    i = i + 1;
end


plot(force, )

function answer = distance(force, friction)
    mass = 60;
    d = 8;
    g = 9.8;
    angle = pi/6;
    answer = sqrt(2*d*(force-mass*g*(sin(angle)+friction*cos(angle)))/mass);
end