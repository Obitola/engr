clear;
clc;
DOsat = 9;
k1 = 0.2;
k2 = 0.4;
li = 25;
di = 4;
e = esp(1)

disp('DO in Wabash in ml/L')
disp('Day    DO')
disp('-----------')

days = 1:20
for i in 1:20:
	within(i) = ' ';

for t = days:
	do(t) = finddo(t);
	if do(t) <= 8 && do(t) >= 6
		within(t) = '*';
	
function x = finddo(t)
	x = DOsat -(k1*li/(k2-k1)) * (e.^(-k1*t)-e^(-k2*t)) - di*e.^(-k2*t);
end
