clc
clear all

force = 480:600;

for i = [1:121]
    dist1 = (force(i)*8) / (60*9.81*sind(30) + 0.36*60*9.81*cosd(30));
end

for i = [1:121]
    dist2 = (force(i)*8 / (60*9.81*sind(30) + 0.42*60*9.81*cosd(30));
end

plot(force, dist1, force, dist2)
title('Constant Force Exerted vs Maximum Distance of Crate')
xlabel('consta